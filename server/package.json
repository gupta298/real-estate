{
  "name": "real-estate-server",
  "version": "1.0.0",
  "description": "Backend API for real estate website with MLS integration",
  "main": "server.js",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "init-db": "node scripts/init-db.js",
    "sync-mls": "node scripts/sync-mls.js",
    "seed": "node scripts/seed-sample-data.js",
    "simple-seed": "node scripts/simple-seed.js",
    "create-admin": "node scripts/create-admin.js",
    "migrate-videos": "node scripts/migrate-video-schema.js",
    "migrate-thumbnail": "node scripts/migrate-off-market-thumbnail.js",
    "migrate-blog-thumbnail": "node scripts/migrate-blog-thumbnail.js",
    "migrate-all": "node scripts/run-all-migrations.js",
    "seed-all": "npm run simple-seed",
    "setup": "npm run init-db && npm run migrate-all && npm run simple-seed",
    "render-setup": "node scripts/render-db-setup.js",
    "test-db": "node scripts/test-pg-connection.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
